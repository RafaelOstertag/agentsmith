.TH @PACKAGE@_exclude @FILE_SECTION@ 2011
.\"=========================================================
.\" NAME
.\"=========================================================
.SH NAME
\fB@PACKAGE@_exclude\fR \- IP Address exclusion file for @PACKAGE@
.\"=========================================================
.\" SYNTAX
.\"=========================================================
.SH SYNTAX
.nf
<\fIAddress\fR>

<\fIAddress\fR>/<\fIPrefix\fR>

# Comments
.fi
.\"=========================================================
.\" DESCRIPTION
.\"=========================================================
.SH DESCRIPTION

.\"=========================================================
.\" EXAMPLE
.\"=========================================================
.SH EXAMPLE

.nf
192.168.1.0/24
172.16.1.1

.fi

.\"=========================================================
.\" SEE ALSO
.\"=========================================================
.SH SEE ALSO
@PACKAGE@(@ADM_SECTION@), agentsmith.conf(@FILE_SECTION@)
